{
	"name": "@lampa/solidjs-spatial-navigation",
	"version": "1.0.0",
	"description": "SolidJS spatial navigation library",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"type": "module",
	"exports": {
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"require": "./dist/index.cjs"
	},
	"scripts": {
		"dev": "vite serve dev",
		"build": "tsup",
		"prepublishOnly": "npm run build",
		"format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
		"typecheck": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LampaWebDev/solidjs-spatial-navigation.git"
	},
	"homepage": "https://github.com/LampaWebDev/solidjs-spatial-navigation#readme",
	"bugs": {
		"url": "https://github.com/LampaWebDev/solidjs-spatial-navigation/issues"
	},
	"keywords": [
		"solid-js",
		"spatial-navigation"
	],
	"author": "Dmytro Nesteruk <dmytro.nesteruk.13@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/lodash": "^4.14.195",
		"@types/node": "^20.3.0",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard-with-typescript": "^35.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-solid": "^0.12.1",
		"prettier": "^2.8.8",
		"solid-refresh": "^0.5.3",
		"typescript": "^5.1.3",
		"concurrently": "^7.6.0",
		"esbuild": "^0.17.4",
		"esbuild-plugin-solid": "^0.5.0",
		"solid-js": "^1.6.10",
		"tsup": "^6.5.0",
		"tsup-preset-solid": "^0.1.4",
		"vite": "^4.0.4",
		"vite-plugin-solid": "^2.5.0"
	},
	"peerDependencies": {
		"solid-js": "^1.6.10"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"browser": {},
	"typesVersions": {}
}
